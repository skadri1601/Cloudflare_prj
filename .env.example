# Copy this to .env and fill in your values
# Note: These are for local development only
# For production, use: wrangler secret put WEATHER_API_KEY

# Weather API (get from https://openweathermap.org/api)
WEATHER_API_KEY=your_openweathermap_api_key_here

# Optional: Events API keys
EVENTS_API_KEY=your_events_api_key_here

# Optional: Custom domain for production
CUSTOM_DOMAIN=travel.yourdomain.com